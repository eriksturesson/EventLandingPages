{
   "compilerOptions": {
      "target": "ES2020",
      "module": "commonjs",
      "moduleResolution": "Node",
      "strict": true,
      "esModuleInterop": true,
      "skipLibCheck": true, // Rekommenderas med vissa @types
      "forceConsistentCasingInFileNames": true,
      "rootDir": "src",
      "outDir": "lib"
   },
   "include": ["src"]
}
